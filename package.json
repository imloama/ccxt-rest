  "scripts": {
    "start": "node app.js",
    "test": "./node_modules/.bin/swagger project test",
    "generate:client": "rm -rf ./clients/$lang && docker run --rm -v ${PWD}:/local swaggerapi/swagger-codegen-cli generate --api-package ph.adroit.ccxtrest.api --model-package ph.adroit.ccxtrest.model --invoker-package ph.adroit.ccxtrest.invoker --group-id ph.adoit.ccxtrest --artifact-id ccxt-rest-$lang-client --artifact-version $npm_package_version -c /local/config.json --additional-properties scmConnection=\"scm:git:git@github.com:franz-see/ccxt-rest-$lang-client.git\" --additional-properties scmDeveloperConnection=\"scm:git:git@github.com:franz-see/ccxt-rest-$lang-client.git\" --additional-properties scmUrl=\"https://github.com/franz-see/ccxt-rest-$lang-client\" --additional-properties artifactUrl=\"https://github.com/franz-see/ccxt-rest-$lang-client\" --additional-properties artifactDescription=\"ccxt-rest $lang client\" -i /local/api/swagger/swagger.yaml -l $lang -o /local/clients/$lang",
    "generate:clients": "for lang in `cat codegen_languages.txt`; do lang=$lang npm run generate:client; done",
    "generate:docs": "./node_modules/.bin/widdershins --search true --language_tabs 'shell:Shell' 'go:Go' 'java:Java' 'javascript:Javascript' 'python:Python' 'ruby:Ruby' --summary api/swagger/swagger.yaml -o ./docs/source/index.html.md && cd ./docs && bundle exec middleman build --clean && cd .. && rm -rf ./out/docs && mv ./docs/build ./out/docs",
    "swagger:edit": "./node_modules/.bin/swagger project edit"
  },
  "bin": {
    "ccxt-rest": "./bin/www"
  }
